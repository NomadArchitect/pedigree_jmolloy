#!/bin/bash

qemu-system-x86_64 \
    -drive file=./build/hdd.img,if=ide,media=disk,cache=none,aio=native,index=0 \
    -drive file=./build/pedigree.iso,if=ide,media=cdrom,cache=none,aio=native,index=2 \
    -boot d -vga std -serial file:build/serial.txt -serial file:build/memlog.txt \
    -enable-kvm -rtc base=utc $*
